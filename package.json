{
  "name": "tsnew",
  "version": "1.0.4",
  "description": "Create a new Typescript project.",
  "license": "Unlicense",
  "author": "Chris Ackerman",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shakeskeyboarde/tsnew.git"
  },
  "bin": {
    "tsnew": "lib/index.js"
  },
  "scripts": {
    "clean": "del-cli lib dist coverage",
    "test": "del-cli coverage && tsc && eslint src && jest --passWithNoTests",
    "build": "del-cli lib && babel src --out-dir lib --extensions .ts,.tsx --source-maps",
    "prestart": "npm run build",
    "start": "node lib/index.js",
    "prepack": "npm test && npm run build"
  },
  "engines": {
    "node": ">=12",
    "npm": ">= 7",
    "git": ">=2.28",
    "gh": ">=1.5"
  },
  "browserslist": [
    "node >= 12"
  ],
  "dependencies": {
    "execa": "^4.1.0",
    "fs-extra": "^9.1.0",
    "latest-version": "^5.1.0",
    "open": "^7.4.2",
    "prompts": "^2.4.0",
    "semver": "^7.3.4",
    "source-map-support": "^0.5.19",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.0",
    "@types/prettier": "^2.2.1",
    "@types/prompts": "^2.0.9",
    "@types/semver": "^7.3.4",
    "@types/source-map-support": "^0.5.3",
    "@types/validate-npm-package-name": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-const-enum": "^1.0.1",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
